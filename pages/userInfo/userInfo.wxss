.info-title {
  height: 48rpx;
  line-height: 48rpx;
  font-family: PingFangSC;
  font-size: 34rpx;
  font-weight: 500;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: left;
  color: rgba(33, 33, 33, 0.87);
  width: 100vw;
  border-bottom: 1rpx solid #d8d8d8;
  padding: 30rpx;
}
.info-cell {
